		<identifier>true</identifier>
		<header>Accession</header>
		<property>gb_primary_accession</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Subtype</header>
		<property>subtype</property>
	</textFileColumn>
	<textFileColumn>
		<header>Country</header>
		<property>country</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Reference</header>
		<property>reference</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>
	<textFileColumn>
		<header>Donor-Recipient</header>
		<property>donor-recipient</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 
	</textFileColumn>
	<textFileColumn>
		<header>Sex</header>
		<property>sex</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Sample_year</header>
		<property>sample_year</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	<textFileColumn>
		<header>Known_transmission_link</header>
		<property>known_transmission_link</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
</textFilePopulator>
